<h1>Listing call_histories</h1>

<table>
  <tr>
    <th>Ticket</th>
    <th>Wicket</th>
    <th>Call history type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @call_histories.each do |call_history| %>
  <tr>
    <td><%= call_history.ticket_id %></td>
    <td><%= call_history.wicket_id %></td>
    <td><%= call_history.call_history_type_id %></td>
    <td><%= link_to 'Show', call_history %></td>
    <td><%= link_to 'Edit', edit_call_history_path(call_history) %></td>
    <td><%= link_to 'Destroy', call_history, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

