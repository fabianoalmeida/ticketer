<!DOCTYPE html>
<html>
<head>
  <title>Ticketer</title>

  <%  if request.env['HTTP_USER_AGENT'] =~ /MSIE/ %>
    <%= stylesheet_link_tag :defaults_ie %>
  <% elsif %>
    <%= stylesheet_link_tag :defaults, :forms %>
  <% end %>

  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
    <div id="page">
      <div class="header">
        <h1 class="logo">Ticketer</h1> 
        <div id="menu">
          <ul>
	    <li><a class="active" title="Inicio" href="#">Inicio</a></li>
	    <li><a title="Cadastros" href="#">Cadastros</a></li>
	    <li><a title="Relatorios" href="#">Relatorios</a></li>
	    <li><a title="Guiches" href="#">Guiches</a></li>
	    <li><a title="Usuario" href="#">Usuario</a></li>
          </ul>
        </div>
      </div>
      <div id="main">
        <div class="login-holder">
          <div class="login infos">
             <span class="user">     Mariazinha de Jesus Silva      </span>
             <a class="logout link" href="#"> Sair</a>
             <img height="24px" src="/images/log.png" class="logout" widht="24px"> 
          </div>
          <div class="triangle"></div>
        </div>
        <%= yield %>
      </div>
    <div class="footer">
    </div>
  </body>
</html>
