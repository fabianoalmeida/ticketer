<div class="form">
  <fieldset>
    <legend><%= t('model.panel.name') %></legend>
    <% if notice %>
      <span class="form-msg-success"><%= notice %></span>
    <% end %>
    <%= render :partial => 'form', :locals => {:disabled => true } %>
  </fieldset>
</div>
<div class="buttons-nav">
  <%= link_to t('model.edit'), edit_place_panel_path(@panel.place, @panel), :class => "button medium" %>
  <%= link_to t('model.back'), place_panels_path(@panel.place), :class => "button medium" %>
</div>
