<div class="totem_group">
<%= form_for( Ticket.new ) do |f| %>
  <%= f.hidden_field :totem_id, :value => params[:totem_id] %>
  <%= f.hidden_field :place_id, :value => params[:place_id] %>
  <% @ticket_type_groups.each do |ticket_type_group| %>
    <%= link_to place_totem_ticket_type_group_generate_ticket_path(params[:place_id], params[:totem_id], ticket_type_group.id) do %>
      <div class="ticket_type_groups generate"><span><%= ticket_type_group.value %></span></div>
    <% end %>
  <% end %>
<% end %>
</div>

<%= javascript_include_tag 'jquery.jclock' %>
</div>
